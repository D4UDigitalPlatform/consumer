<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="consumer.hydrator.array.class">Itkg\Consumer\Hydrator\Simple</parameter>
        <parameter key="consumer.hydrator.json.class">Itkg\Consumer\Hydrator\Json</parameter>
        <parameter key="consumer.hydrator.xml.class">Itkg\Consumer\Hydrator\Xml</parameter>
    </parameters>
    <services>
        <service id="consumer.hydrator.array" class="%consumer.hydrator.array.class%" />
        <service id="consumer.hydrator.json" class="%consumer.hydrator.json.class%" />
        <service id="consumer.hydrator.xml" class="%consumer.hydrator.xml.class%" />
    </services>

</container>