<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="consumer.service.class">Itkg\Consumer\Service</parameter>
        <parameter key="consumer.service.factory.class">Itkg\Consumer\Service\Method\Factory</parameter>
    </parameters>
    <services>
        <service id="consumer.service" class="%consumer.service.class%">
            <call method="setEventDispatcher">
                <argument type="service" id="core.event_dispatcher" />
            </call>
            <call method="setCacheManager">
                <argument type="service" id="cache.manager" />
            </call>
        </service>
    </services>
</container>