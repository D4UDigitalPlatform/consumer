<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="consume.service.event.filter_service_event.class">Itkg\Consume\Service\Event\FilterServiceEvent</parameter>
        <parameter key="consume.service.event.service_log_subscriber.class">Itkg\Consume\Service\Event\ServiceLogSubscriber</parameter>
        <parameter key="consume.service.event.service_debug_subscriber.class">Itkg\Consume\Service\Event\ServiceDebugSubscriber</parameter>
    </parameters>

    <services>
        <service id="consume.service.event.filter_service_event" class="%consume.service.event.filter_service_event.class%" public="true"/>
        <service id="consume.service.event.service_log_subscriber" class="%consume.service.event.service_log_subscriber.class%">
            <tag name="core.subscriber"/>
        </service>
     <!--   <service id="consume.service.event.service_debug_subscriber" class="%consume.service.event.service_debug_subscriber.class%">
            <tag name="core.subscriber"/>
        </service> -->
    </services>
</container>