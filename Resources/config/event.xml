<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="consume.event.filter_service_event.class">Itkg\Consume\Event\FilterServiceEvent</parameter>
        <parameter key="consume.event.service_subscriber.class">Itkg\Consume\Event\ServiceSubscriber</parameter>
    </parameters>

    <services>
        <service id="consume.event.filter_service_event" class="%consume.event.filter_service_event.class%" public="true"/>
        <service id="consume.event.service_subscriber" class="%consume.event.service_subscriber.class%">
            <tag name="core.subscriber"/>
        </service>
    </services>

</container>